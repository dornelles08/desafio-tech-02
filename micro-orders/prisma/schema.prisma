generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id             String @id @default(uuid())
  status         String
  value          Int
  customer_name  String
  customer_email String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("orders")
}
